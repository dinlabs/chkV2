<item>
{% apply spaceless %}
{% apply setono_sylius_feed_remove_empty_tags %}
    <g:id>{{ item.id|e }}</g:id>
    <title>{{ item.title|e }}</title>
    <g:description>{{ item.description|e }}</g:description>
    <g:product_type>{{ item.productType|e }}</g:product_type>
    <link>{{ item.link|e }}</link>
    <g:image_link>{{ item.imageLink|e }}</g:image_link>
    {% for additionalImageLink in item.additionalImageLinks %}
        <g:additional_image_link>{{ additionalImageLink|e }}</g:additional_image_link>
    {% endfor %}
    <g:condition>{{ item.condition|e }}</g:condition>
    <g:availability>{{ item.availability|e }}</g:availability>
    <g:price>{{ item.price|e }}EUR</g:price>
    <g:sale_price>{{ item.salePrice|e }}EUR</g:sale_price>
    <g:sale_price_effective_date>{{ item.salePriceEffectiveDate|e }}EUR</g:sale_price_effective_date>
    <g:brand>{{ item.brand|e }}</g:brand>
    <g:gtin>{{ item.gtin|e }}</g:gtin>
    <g:mpn>{{ item.mpn|e }}</g:mpn>
    <g:identifier_exists>TRUE</g:identifier_exists>
    {#<g:gender>{gender}</g:gender>
    <g:age_group>{age_group}</g:age_group>#}
    <g:color>{{ item.color|e }}</g:color>
    <g:size>{{ item.size|e }}</g:size>
    {#G:ITEM_GROUP_ID}
    <g:material>{material}</g:material>
    <g:pattern>{pattern}</g:pattern>
    <g:shipping_weight>{weight,[float],[2]}kg</g:shipping_weight>#}
    <!-- AdWords attributes -->
    {#<g:custom_label_1>{article_centralise}</g:custom_label_1>
    <g:custom_label_2>{cycle_vie}</g:custom_label_2>
    <g:adwords_grouping>{adwords_grouping}</g:adwords_grouping>#}
    {% for idx, customLabel in item.customLabels %}
        <g:custom_label_{{ idx }}>{{ customLabel|e }}</g:custom_label_{{ idx }}>
    {% endfor %}
{% endapply %}
{% endapply %}
</item>